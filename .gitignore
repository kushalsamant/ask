# Python bytecode and build artifacts
__pycache__/  # Python cache directories
*.py[cod]  # Python compiled files
*$py.class  # Python class files
*.so  # Shared object files
.Python  # Python executable
build/  # Build directory
develop-eggs/  # Development eggs
dist/  # Distribution directory
downloads/  # Downloads directory
eggs/  # Eggs directory
.eggs/  # Hidden eggs directory
lib/  # Library directory
lib64/  # 64-bit library directory
parts/  # Parts directory
sdist/  # Source distribution
var/  # Variable directory
wheels/  # Wheel packages
*.egg-info/  # Egg info directories
.installed.cfg  # Installation configuration
*.egg  # Egg packages

# Virtual environments
venv/  # Virtual environment directory
env/  # Environment directory
ENV/  # Environment directory (uppercase)

# Environment files (contains API keys)
.env  # Environment variables file
.env.local  # Local environment file
.env.production  # Production environment file

# Logs
logs/  # Log directory
*.log  # All log files
log.csv  # Questions and images log
logs/execution.log  # Main application execution log



# Temporary files
temp_*  # Temporary files
*.tmp  # Temporary files
*.temp  # Temporary files

# Generated content
daily_pdfs/*.pdf  # Generated PDF files
images/  # Generated image directory
captions/*/  # Generated caption directories
instagram/*/  # Generated Instagram content

# API tokens
*.token  # API token files

# IDE
.vscode/  # Visual Studio Code settings
.idea/  # IntelliJ IDEA settings
*.swp  # Vim swap files
*.swo  # Vim swap files

# OS
.DS_Store  # macOS system files
Thumbs.db  # Windows thumbnail cache

# Large files
*.pdf  # PDF files
*.zip  # ZIP archives
*.tar.gz  # Compressed archives

# Backup files
*.bak  # Backup files
*.backup  # Backup files
